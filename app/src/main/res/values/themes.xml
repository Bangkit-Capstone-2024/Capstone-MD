<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MoMee" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorSecondary">@color/color_secondary</item>
        <item name="colorTertiary">@color/color_tertiary</item>
    </style>


    <style name="Theme.MoMee" parent="Base.Theme.MoMee">
        <item name="android:windowBackground">#FCFCFC</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">@dimen/font_14</item>
    </style>

    <style name="Widget.App.Button.GoogleButtonStyle" parent="Widget.Material3.Button">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Button.GoogleButtonStyle</item>
        <item name="strokeColor">@color/google_btn_stroke_day</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="ThemeOverlay.App.Button.GoogleButtonStyle" parent="ThemeOverlay.Material3.Button">
        <item name="colorContainer">@android:color/white</item>
        <item name="colorOnContainer">@color/google_btn_font_day</item>
    </style>
</resources>